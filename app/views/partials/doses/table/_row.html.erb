<tr>

  <td>
    <%= dose.kind %>
  </td>

  <td>
    <%= dose.quantity %>
  </td>

  <% if current_user.admin? %>
    <td>
      <%= link_to 'Editar', edit_dose_path(dose) %>
    </td>

    <td><%= link_to 'Eliminar', dose_path(dose),
	method: :delete,
	data: { confirm: 'Estas Seguro?' } %>
    </td>
  <% end %>
</tr>
