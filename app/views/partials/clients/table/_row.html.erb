
 <tr>

  <td>
    <%= link_to client.name, client_path(client) %>
  </td>
  
  <td>
    <%= client.lastname %>
  </td>
  
  <td>
    <%= client.rut %>
  </td>
  
  <td>
    <%= client.email %>
  </td>
  
  <td>
    <%= client.address %>
  </td>
  
  
   <td>
    <%= client.gender %>
  </td>
  
   <td>
    <%= client.birth %>
  </td>
  
   <td>
    <%= client.kind %>
  </td>
  
   <td>
    <%= link_to 'Editar', edit_client_path(client) %>
   </td>
  
  <td>
       <%= link_to 'Eliminar', client_path(client),
              method: :delete,
              data: { confirm: 'Estas Seguro?' } %>
  </td>
  
</tr>
