<div class="panel panel-default">
  <div class="panel-heading">
    Buscar Presentation
  </div>

  <div class="panel-body">
    <%= form_tag(search_presentations_path, method: :get, remote: true) do %>
      <%= autocomplete_field_tag :search_presentation_field, '', autocomplete_presentation_name_presentations_path %>
      <%= submit_tag('Buscar', class: 'btn btn-default') %>
    <% end %>
    
    
    <div id="search_presentation_result">
    </div>
  </div>
</div>
