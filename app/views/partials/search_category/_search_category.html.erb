<div class="panel panel-default">
  <div class="panel-heading">
    Buscar Categorias
  </div>

  <div class="panel-body">
    <%= form_tag(search_categories_path, method: :get, remote: true) do %>
      <%= autocomplete_field_tag :search_category_field, '', autocomplete_category_description_categories_path %>
      <%= submit_tag('Buscar', class: 'btn btn-default') %>
    <% end %>
    
    
    <div id="search_category_result">
    </div>
  </div>
</div>
