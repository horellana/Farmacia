<div class="panel panel-default">
  <div class="panel-heading">
    Buscar Dosis
  </div>

  <div class="panel-body">
    <%= form_tag(search_doses_path, method: :get, remote: true) do %>
      <%= autocomplete_field_tag :search_dose_field, '', autocomplete_dose_kind_doses_path %>
      <%= submit_tag('Buscar', class: 'btn btn-default') %>
    <% end %>
    
    
    <div id="search_dose_result">
    </div>
  </div>
</div>
