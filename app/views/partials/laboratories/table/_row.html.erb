<tr>

  <td>
    $ <%= laboratory.name %>
  </td>

  <% if current_user.admin? %>
    <td>
      <%= link_to 'Editar', edit_laboratory_path(laboratory) %>
    </td>
  <% end %>

  <% if current_user.admin? %>
    <td><%= link_to 'Eliminar', laboratory_path(laboratory),
	method: :delete,
	data: { confirm: 'Estas Seguro?' } %>
    </td>
  <% end %>

</tr>
