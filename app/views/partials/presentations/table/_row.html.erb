<tr>

  <td>
    <%= presentation.name %>
  </td>


  <% if current_user.admin? %>
    <td>
      <%= link_to 'Editar', edit_presentation_path(presentation) %>
    </td>

    <td><%= link_to 'Eliminar', presentation_path(presentation),
	method: :delete,
	data: { confirm: 'Estas Seguro?' } %>
    </td>
  <% end %>
</tr>
