<div class="panel panel-default">
  <div class="panel-heading">
    Iniciar sesion
  </div>

  <div class="panel-body">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <div class="field">
	<%= f.label :rut %></br>
	<%= f.text_field :rut, autofocus: true, class:"form-control" %>
      </div>

      <br>

      <div class="field">
	<%= f.label :password %></br>
	<%= f.password_field :password, autocomplete: "off" , class:"form-control"%>
      </div>

      <br>

      <div class="acctions">
	<%= f.submit "Iniciar Sesion",class: 'btn btn-default' %>
      </div>
    <% end %>
  </div>
</div>
