<div class="panel panel-default">
  <div class="panel-heading">
  </div>

  <table class="table">
      <thead>
        <th>
 Principio activo
      </th>

      <th>
	Nombre
      </th>

      <th>
	Presentacion
      </th>

      <th>
  Precio por unidad
      </th>

      <th>
  BE
  </th>
    </thead>
    <tbody>
      <% @products.each do |pr| %>
      <tr>
        <td>
        <%=  pr.principles.first.name %>
        </td>

        <td>
        <%= pr.name  %>
        </td>

       <td>
        <%= pr.presentation.name %>
        </td>

        <td>
      <%=  pr.sale_price  %>
